<Window x:Class="LoveCalculatorApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LoveCalculatorApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <StackPanel>
            <DatePicker x:Name="DatePicker1"></DatePicker>
            <DatePicker x:Name="DatePicker2"></DatePicker>
            <Button Click="Match_OnClick">Match</Button>
            <TextBox x:Name="TextBoxResult"></TextBox>
        </StackPanel>
    </Grid>
</Window>
